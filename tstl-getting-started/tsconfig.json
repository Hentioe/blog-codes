{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "module": "commonjs",
    "moduleResolution": "Node",
    "types": [
      "lua-types/5.4"
    ],
    "strict": true,
    "outDir": "dist",
    "rootDir": "src"
  },
  "tstl": {
    "luaTarget": "5.4"
  }
}